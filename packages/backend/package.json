{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "description": "Express TypeScript backend for Experience Miner with Gemini AI integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsup",
    "start": "node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "check": "biome check --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@google/genai": "^1.10.0",
    "@supabase/supabase-js": "^2.49.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "multer": "^2.0.2",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.9",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "pino-pretty": "^13.0.0",
    "supertest": "^7.1.3",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "outDir": "dist",
    "format": [
      "esm"
    ],
    "target": "es2020",
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "splitting": false,
    "skipNodeModulesBundle": true
  },
  "engines": {
    "node": ">=18.0.0"
  }
}